{
  "extends": "../tsconfig.strict.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "noEmitOnError": true,
    "sourceMap": true,
    "lib": [
      "ES2019",
      "DOM"
    ],
    "target": "ESNext",
    "module": "Node16",
    "moduleResolution": "Node16",
    "noPropertyAccessFromIndexSignature": true
  }
}
